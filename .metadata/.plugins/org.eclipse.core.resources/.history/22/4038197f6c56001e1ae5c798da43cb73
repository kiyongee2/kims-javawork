package collection.map;

import java.util.HashMap;
import java.util.Map;

public class HashMapTest {

	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<>();
		
		map.put("이순신", 95);
		map.put("홍길동", 75);
		map.put("강감찬", 85);
		
		System.out.println(map.get("홍길동"));
	}

}
